{
  "name": "cloudhound",
  "version": "0.1.1",
  "description": "Find objects in the cloud",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "nodeVersion": ">= 12.0.0",
  "scripts": {
    "jsdoc": "jsdoc -c ./docconfig/jsdoc.json",
    "build": "tsc",
    "watch": "tsc --watch",
    "clean": "rm -fr lib/*",
    "prepublish": "npm run build",
    "prepublishOnly": "tsc -p ./ --outDir dist",
    "test": "mocha -r ts-node/register test/*.ts",
    "posttest": "npm run lint",
    "lint": "eslint . --ext .ts --ext .json",
    "lint:fix": "eslint . --ext .ts --fix",
    "coverage": "nyc mocha && nyc report --reporter=html && nyc report --reporter=json-summary",
    "no-npm": "no-npm publish --repo git@github.com:bbc/icat-s3-hound.git"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nspragg/cloudhound.git"
  },
  "keywords": [
    "aws",
    "s3",
    "file",
    "cloud",
    "recursive",
    "search",
    "cloudhound",
    "file",
    "hound",
    "lister",
    "find",
    "walk",
    "walker",
    "finder",
    "file system"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nspragg/cloudhound/issues"
  },
  "homepage": "https://github.com/nspragg/cloudhound",
  "devDependencies": {
    "@types/chai": "^4.2.12",
    "@types/lodash": "^4.14.161",
    "@types/mocha": "^2.2.43",
    "@types/node": "^10.12.18",
    "@types/sinon": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "eslint": "^7.26.0",
    "eslint-config-iplayer": "^8.0.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-mocha": "^10.0.5",
    "jsdoc": "^3.6.10",
    "mocha": "^10.0.0",
    "no-npm": "github:bbc/no-npm#semver:^2.0.0",
    "nyc": "^15.1.0",
    "sinon": "^2.3.6",
    "ts-node": "^7.0.1",
    "typescript": "^4.0.0"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "dependencies": {
    "aws-sdk": "^2.823.0",
    "lodash": "^4.17.21"
  }
}
